{
  "name": "ascii-calendar",
  "version": "0.2.1",
  "description": "Simple ASCII Calendar similar to Linux cal command in JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "browserify -s cal index.js | uglifyjs -c --mangle 'reserved=cal' > dist/umd.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcubic/calendar.git"
  },
  "keywords": [
    "ascii",
    "terminal",
    "cli",
    "text",
    "calendar"
  ],
  "author": "Jakub T. Jankiewicz <jcubic@onet.pl> (https://jcubic.pl/jakub-jankiewicz/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcubic/calendar/issues"
  },
  "homepage": "https://github.com/jcubic/calendar#readme",
  "devDependencies": {
    "browserify": "^17.0.1",
    "uglify-js": "^3.19.3"
  }
}
